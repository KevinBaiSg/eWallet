[ignore]
<PROJECT_ROOT>/app/main.prod.js
<PROJECT_ROOT>/app/main.prod.js.map
<PROJECT_ROOT>/app/dist/.*
<PROJECT_ROOT>/resources/.*
<PROJECT_ROOT>/node_modules/webpack-cli
<PROJECT_ROOT>/node_modules/protobufjs-old-fixed-webpack
<PROJECT_ROOT>/node_modules/trezor-link/lib/bridge
<PROJECT_ROOT>/node_modules/trezor-link/lib/lowlevel/send.js.flow
<PROJECT_ROOT>/node_modules/trezor-link/lib/lowlevel/webusb.js.flow
<PROJECT_ROOT>/node_modules/trezor-link/lib/lowlevel/withSharedConnections.js.flow
<PROJECT_ROOT>/node_modules/trezor-link/lib/device-list.js.flow
<PROJECT_ROOT>/node_modules/trezor-link/lib/fallback.js.flow
<PROJECT_ROOT>/node_modules/trezor-link/lib/index.js.flow
<PROJECT_ROOT>/node_modules/trezor.js/lib/session.js.flow
<PROJECT_ROOT>/node_modules/trezor.js/lib/device-list.js.flow
<PROJECT_ROOT>/node_modules/trezor.js/lib/device.js.flow
<PROJECT_ROOT>/node_modules/trezor.js/lib/utils/hdnode.js.flow
<PROJECT_ROOT>/release/.*
<PROJECT_ROOT>/dll/.*
<PROJECT_ROOT>/release/.*
<PROJECT_ROOT>/git/.*
<PROJECT_ROOT>/flowtype/.*

[include]

[libs]

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=enable
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/internals/flow/CSSModule.js.flow'
module.name_mapper.extension='styl' -> '<PROJECT_ROOT>/internals/flow/CSSModule.js.flow'
module.name_mapper.extension='scss' -> '<PROJECT_ROOT>/internals/flow/CSSModule.js.flow'
module.name_mapper.extension='png' -> '<PROJECT_ROOT>/internals/flow/WebpackAsset.js.flow'
module.name_mapper.extension='jpg' -> '<PROJECT_ROOT>/internals/flow/WebpackAsset.js.flow'
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
module.name_mapper='^views' ->'<PROJECT_ROOT>/app/views'
module.name_mapper='^components' ->'<PROJECT_ROOT>/app/components'
module.name_mapper='^config' ->'<PROJECT_ROOT>/app/config'
module.name_mapper='^constants' ->'<PROJECT_ROOT>/app/constants'
module.name_mapper='^utils' ->'<PROJECT_ROOT>/app/utils'
module.name_mapper='^store' ->'<PROJECT_ROOT>/app/store'
module.name_mapper='^flowtype' ->'<PROJECT_ROOT>/flowtype'
